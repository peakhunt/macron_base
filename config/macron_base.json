{
  "schema": {
    "version": 1,
    "last_update": "Mar/30/2018"
  },

  "cli": {
    "prompt": "macron_bog> ",
    "telnet": {
      "enabled": true,
      "port": 9090
    },
    "serial": {
      "enabled": false,
      "serial_port": "/dev/ttyUSB0",
      "baud": 115200,
      "data_bit": 8,
      "stop_bit": 1,
      "parity": "none"
    }
  },

  "modbus_slave_list" : [
    {
      "protocol": "tcp",
      "address" : 15,
      "param" : {
        "port"    : 8080
      }
    },
    {
      "protocol": "rtu",
      "address" : 16,
      "param" : {
        "serial_port": "/dev/ttyUSB0",
        "baud": 115200,
        "data_bit": 8,
        "stop_bit": 1,
        "parity": "none"
      }
    }
  ]
}
