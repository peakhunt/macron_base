#!/usr/bin/expect

set test_cnt 0

proc do_cli_telnet {} {
  spawn telnet localhost 9090
  expect "macron_bog> "

  send "help\r"

  interact timeout 1 return

  send ""
  expect "telnet> "
  send "q\r"
  expect eof
  wait
}

set timeout 20

while { 1 } {
  do_cli_telnet

  set test_cnt  "[expr $test_cnt + 1]"
  puts "######### Test $test_cnt passed"
  sleep 1
}
